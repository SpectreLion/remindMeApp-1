{
  "name": "RemindMe",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest"
  },
  "rnpm": {
    "assets": [
      "assets/fonts/Avenir"
    ]
  },
  "dependencies": {
    "react": "16.2.0",
    "react-native": "0.52.2",
    "react-native-datepicker": "^1.6.0",
    "react-navigation": "^1.0.0-beta.29"
  },
  "devDependencies": {
    "babel-jest": "22.1.0",
    "babel-preset-react-native": "4.0.0",
    "jest": "22.1.4",
    "mockdate": "^2.0.2",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "16.2.0"
  },
  "jest": {
    "preset": "react-native",
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "moduleNameMapper": {
      "^image![a-zA-Z0-9$_-]+$": "GlobalImageStub",
      "^[./a-zA-Z0-9$_-]+\\.png|[./a-zA-Z0-9$_-]+\\.jpg$": "<rootDir>/jestConfig/RelativeImageStub.js"
    }
  }
}
